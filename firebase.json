{
  "hosting": {
    "public": "www",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source":"/android-app",
        "destination": "/android.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
